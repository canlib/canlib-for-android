<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="example.canlib"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="21" />
    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:name="example.canlib.MyApplication"
        android:allowBackup="true"
        android:icon="@drawable/canlib_logo"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".SettingNameActivity"
            android:label="@string/app_name"
            android:onClick="SetButtonOnClick"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
                <activity
            android:name=".ListSelectorActivity"
            android:label="@string/app_name"
            android:configChanges="orientation|screenSize" />
        <activity android:name=".UserBooksListFragment"></activity>
        <activity android:name=".OtherBooksListFragment"></activity>
    </application>

</manifest>
